    %vpi_call 2 13 "$monitor", "%b\011 %b\011 %b %b %b %b", v0xd11a80_0, v0xd11b20_0, v0xd11bd0_0, v0xd11c80_0, v0xd11d60_0, v0xd11e10_0;
