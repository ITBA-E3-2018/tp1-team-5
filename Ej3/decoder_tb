v0xd00af0_0 .net "a", 0 0, L_0xd11e90; 1 drivers
v0xd114e0_0 .net "b", 0 0, L_0xd120c0; 1 drivers
v0xd11580_0 .net "c", 0 0, L_0xd12220; 1 drivers
v0xd11620_0 .net "inp1", 0 0, v0xd11a80_0; 1 drivers
v0xd116d0_0 .net "inp2", 0 0, v0xd11b20_0; 1 drivers
v0xd11770_0 .alias "out1", 0 0, v0xd11bd0_0;
v0xd11850_0 .alias "out2", 0 0, v0xd11c80_0;
v0xd118f0_0 .alias "out3", 0 0, v0xd11d60_0;
v0xd119e0_0 .alias "out4", 0 0, v0xd11e10_0;
