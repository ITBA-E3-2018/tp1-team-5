    %set/v v0xd11b20_0, 0, 1;
    %delay 10, 0;
    %set/v v0xd11a80_0, 0, 1;
    %set/v v0xd11b20_0, 1, 1;
    %delay 10, 0;
    %set/v v0xd11a80_0, 1, 1;
    %set/v v0xd11b20_0, 1, 1;
    %delay 10, 0;
    %vpi_call 2 31 "$finish";
    %end;
