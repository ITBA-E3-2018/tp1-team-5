L_0xd11e90 .functor OR 1, v0xd11a80_0, v0xd11b20_0, C4<0>, C4<0>;
L_0xd11fc0 .functor NOT 1, L_0xd11e90, C4<0>, C4<0>, C4<0>;
L_0xd120c0 .functor NOT 1, v0xd11b20_0, C4<0>, C4<0>, C4<0>;
L_0xd12120 .functor AND 1, L_0xd120c0, v0xd11a80_0, C4<1>, C4<1>;
L_0xd12220 .functor NOT 1, v0xd11a80_0, C4<0>, C4<0>, C4<0>;
L_0xd12310 .functor AND 1, L_0xd12220, v0xd11b20_0, C4<1>, C4<1>;
